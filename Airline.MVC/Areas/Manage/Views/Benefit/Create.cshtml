@using Airline.Business.ViewModel.BenefitVM;
@model BenefitCreateVM

<div class=" container">


    <form method="post">
        <div class="form-group">
            <label asp-for="Title">Title</label>
            <input class="form-control" asp-for="Title">
            <span asp-validation-for="Title" style=" color: red"></span>
        </div>
        <div class="form-group">
            <label asp-for="Description"> Description</label>
            <input class="form-control" asp-for="Description">
            <span asp-validation-for="Description" style=" color: red"></span>
        </div>
        <div class="row">
            <div class="mb-3" col-md-3 style="margin-right: 30px;">
                <label asp-for="Icon" class="form-label">Icon</label>
                <input asp-for="Icon" class="form-control">
                <span asp-validation-for="Icon" style="color: red"></span>
            </div>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>
